func transition_fade(scene:String):
	get_node("AnimationPlayer").play("fade")
	yield(get_node("AnimationPlayer"),"animation_finished")
	get_tree().change_scene(scene)
	get_node("AnimationPlayer").play_backwards("fade")
	
func fade():
	get_node("AnimationPlayer").play("fade")
	yield(get_node("AnimationPlayer"),"animation_finished")
	get_node("AnimationPlayer").play_backwards("fade")
	yield(get_node("AnimationPlayer"),"animation_finished")
	fade()
